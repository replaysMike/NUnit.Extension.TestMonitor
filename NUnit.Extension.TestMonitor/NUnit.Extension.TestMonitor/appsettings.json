{
  "TestMonitor": {
    // the type of events to emit, can define multiple. Valid options: NamedPipes,StdOut,LogFile
    "EventEmitType": "NamedPipes,StdOut",
    // format of the event data. Valid options: json, xml
    "EventFormat": "json",
    // full path to save the log to
    "EventsLogFile": "",
    // for EventEmitType=StdOut, choose which stream to use. Valid options: StdOut, Trace, Debug, TestContext, None
    "EventOutputStream": "Debug",
    // The timeout (in seconds) to wait for a Named Pipe client connection
    "NamedPipesConnectionTimeoutMilliseconds": 2000
  }
}